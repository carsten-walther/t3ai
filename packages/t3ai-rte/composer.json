{
    "name": "carsten-walther/t3ai-rte",
    "type": "typo3-cms-extension",
    "description": "GenAI for TYPO3 RichTextEditor",
    "homepage": "https://carstenwalther.de",
    "license": ["GPL-2.0-or-later"],
    "authors": [{
        "name": "Carsten Walther",
        "email": "walther.carsten@web.de",
        "role": "Developer"
    }],
    "support": {
        "chat": "",
        "docs": "",
        "issues": "",
        "source": ""
    },
    "config": {
        "sort-packages": true
    },
    "require": {
        "carsten-walther/t3ai-core": "*"
    },
    "extra": {
        "typo3/cms": {
            "Package": {
                "partOfFactoryDefault": false
            },
            "extension-key": "t3ai_rte"
        }
    },
    "minimum-stability" : "alpha",
    "autoload": {
        "psr-4": {
            "CarstenWalther\\T3aiRte\\": "Classes/"
        }
    }
}
